{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "scripthost",
            "es5",
            "es2015.promise"
        ],
        "sourceMap": true,
        "noImplicitAny": false,
        "preserveConstEnums": false,
        "declaration": false,
        "emitDecoratorMetadata": false,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noEmitHelpers": true,
        "importHelpers": true,
        "removeComments": true,
        "paths": {
            "vue-mapp": ["./node_modules/vue-mapp/es5"]
        }
    },
    "include": [
        "./src/*",
        "./src/**/*",
        "./types/**/*",
        "./node_modules/vue-mapp/ts",
        "./node_modules/vue-mapp/types",
    ]
}
